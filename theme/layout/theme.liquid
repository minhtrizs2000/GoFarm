<!doctype html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    {{ content_for_header }} <!-- Header hook for plugins -->
    {{ 'application.css' | asset_url | stylesheet_tag }}
    {{ 'application.js' | asset_url | script_tag }}
  
    {{ '//cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css' | stylesheet_tag }}
    {{ '//cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js' | script_tag }}
    {{ '//cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js' | script_tag }}

    {{ 'base.css' | asset_url | stylesheet_tag }}
  </head>
<body>
  {% section 'announcement-bar' %}
  <header>
    {% section 'header' %}
  </header>

  <main role="main">
    {% render 'breadcrumb' %}
    {{ content_for_layout }}
  </main>

  <footer> 
    {% section 'footer' %}
  </footer>

</body>
</html>
